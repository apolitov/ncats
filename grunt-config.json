{
  "min": {
    "vendor": {
      "src": ["out/scripts/vendors/jquery.js", "out/scripts/vendors/log.js", "out/scripts/vendors/script.js"],
      "dest": "out/scripts/vendors/vendors.min.js"
    },
    "modernizr": {
      "src": ["out/scripts/vendors/modernizr.js"],
      "dest": "out/scripts/vendors/modernizr.min.js"
    },
    "index": {
      "src": ["out/scripts/index.js"],
      "dest": "out/scripts/index.js"
    }
  },
  "concat": {
